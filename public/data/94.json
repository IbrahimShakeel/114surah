      "name": "سورة الشرح",
      "englishName": "Ash-Sharh",
      "englishNameTranslation": "The Consolation",
      "revelationType": "Meccan",
      "numberOfAyahs": 8,
      "ayahs": [
        {
          "number": 6091,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6091,
          "text": "أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6092,
          "text": "وَوَضَعْنَا عَنْكَ وِزْرَكَ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6093,
          "text": "الَّذِي أَنْقَضَ ظَهْرَكَ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6094,
          "text": "وَرَفَعْنَا لَكَ ذِكْرَكَ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6095,
          "text": "فَإِنَّ مَعَ الْعُسْرِ يُسْرًا",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6096,
          "text": "إِنَّ مَعَ الْعُسْرِ يُسْرًا",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6097,
          "text": "فَإِذَا فَرَغْتَ فَانْصَبْ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6098,
          "text": "وَإِلَىٰ رَبِّكَ فَارْغَبْ",
          "numberInSurah": 8,
          "juz": 30
        }
      ],
      "edition": {
