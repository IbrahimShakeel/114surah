  "data": [
    {
      "number": 88,
      "name": "سورة الغاشية",
      "englishName": "Al-Ghaashiya",
      "englishNameTranslation": "The Overwhelming",
      "revelationType": "Meccan",
      "numberOfAyahs": 26,
      "ayahs": [
        {
          "number": 5968,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 5968,
          "text": "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 5969,
          "text": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 5970,
          "text": "عَامِلَةٌ نَاصِبَةٌ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 5971,
          "text": "تَصْلَىٰ نَارًا حَامِيَةً",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 5972,
          "text": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 5973,
          "text": "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 5974,
          "text": "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 5975,
          "text": "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 5976,
          "text": "لِسَعْيِهَا رَاضِيَةٌ",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 5977,
          "text": "فِي جَنَّةٍ عَالِيَةٍ",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 5978,
          "text": "لَا تَسْمَعُ فِيهَا لَاغِيَةً",
          "numberInSurah": 11,
          "juz": 30
        },
        {
          "number": 5979,
          "text": "فِيهَا عَيْنٌ جَارِيَةٌ",
          "numberInSurah": 12,
          "juz": 30
        },
        {
          "number": 5980,
          "text": "فِيهَا سُرُرٌ مَرْفُوعَةٌ",
          "numberInSurah": 13,
          "juz": 30
        },
        {
          "number": 5981,
          "text": "وَأَكْوَابٌ مَوْضُوعَةٌ",
          "numberInSurah": 14,
          "juz": 30
        },
        {
          "number": 5982,
          "text": "وَنَمَارِقُ مَصْفُوفَةٌ",
          "numberInSurah": 15,
          "juz": 30
        },
        {
          "number": 5983,
          "text": "وَزَرَابِيُّ مَبْثُوثَةٌ",
          "numberInSurah": 16,
          "juz": 30
        },
        {
          "number": 5984,
          "text": "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ",
          "numberInSurah": 17,
          "juz": 30
        },
        {
          "number": 5985,
          "text": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ",
          "numberInSurah": 18,
          "juz": 30
        },
        {
          "number": 5986,
          "text": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ",
          "numberInSurah": 19,
          "juz": 30
        },
        {
          "number": 5987,
          "text": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ",
          "numberInSurah": 20,
          "juz": 30
        },
        {
          "number": 5988,
          "text": "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ",
          "numberInSurah": 21,
          "juz": 30
        },
        {
          "number": 5989,
          "text": "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ",
          "numberInSurah": 22,
          "juz": 30
        },
        {
          "number": 5990,
          "text": "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ",
          "numberInSurah": 23,
          "juz": 30
        },
        {
          "number": 5991,
          "text": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ",
          "numberInSurah": 24,
          "juz": 30
        },
        {
          "number": 5992,
          "text": "إِنَّ إِلَيْنَا إِيَابَهُمْ",
          "numberInSurah": 25,
          "juz": 30
        },
        {
          "number": 5993,
          "text": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ",
          "numberInSurah": 26,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
