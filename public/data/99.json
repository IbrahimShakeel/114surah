    {
      "number": 99,
      "name": "سورة الزلزلة",
      "englishName": "Az-Zalzala",
      "englishNameTranslation": "The Earthquake",
      "revelationType": "Medinan",
      "numberOfAyahs": 8,
      "ayahs": [
        {
          "number": 6139,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6139,
          "text": "إِذَا زُلْزِلَتِ الْأَرْضُ زِلْزَالَهَا",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6140,
          "text": "وَأَخْرَجَتِ الْأَرْضُ أَثْقَالَهَا",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6141,
          "text": "وَقَالَ الْإِنْسَانُ مَا لَهَا",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6142,
          "text": "يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6143,
          "text": "بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6144,
          "text": "يَوْمَئِذٍ يَصْدُرُ النَّاسُ أَشْتَاتًا لِيُرَوْا أَعْمَالَهُمْ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6145,
          "text": "فَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6146,
          "text": "وَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُ",
          "numberInSurah": 8,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
