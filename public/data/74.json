  "code": 200,
  "status": "OK",
  "data": [
    {
      "number": 74,
      "name": "سورة المدثر",
      "englishName": "Al-Muddaththir",
      "englishNameTranslation": "The Cloaked One",
      "revelationType": "Meccan",
      "numberOfAyahs": 56,
      "ayahs": [
        {
          "number": 5496,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5496,
          "text": "يَا أَيُّهَا الْمُدَّثِّرُ",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5497,
          "text": "قُمْ فَأَنْذِرْ",
          "numberInSurah": 2,
          "juz": 29
        },
        {
          "number": 5498,
          "text": "وَرَبَّكَ فَكَبِّرْ",
          "numberInSurah": 3,
          "juz": 29
        },
        {
          "number": 5499,
          "text": "وَثِيَابَكَ فَطَهِّرْ",
          "numberInSurah": 4,
          "juz": 29
        },
        {
          "number": 5500,
          "text": "وَالرُّجْزَ فَاهْجُرْ",
          "numberInSurah": 5,
          "juz": 29
        },
        {
          "number": 5501,
          "text": "وَلَا تَمْنُنْ تَسْتَكْثِرُ",
          "numberInSurah": 6,
          "juz": 29
        },
        {
          "number": 5502,
          "text": "وَلِرَبِّكَ فَاصْبِرْ",
          "numberInSurah": 7,
          "juz": 29
        },
        {
          "number": 5503,
          "text": "فَإِذَا نُقِرَ فِي النَّاقُورِ",
          "numberInSurah": 8,
          "juz": 29
        },
        {
          "number": 5504,
          "text": "فَذَٰلِكَ يَوْمَئِذٍ يَوْمٌ عَسِيرٌ",
          "numberInSurah": 9,
          "juz": 29
        },
        {
          "number": 5505,
          "text": "عَلَى الْكَافِرِينَ غَيْرُ يَسِيرٍ",
          "numberInSurah": 10,
          "juz": 29
        },
        {
          "number": 5506,
          "text": "ذَرْنِي وَمَنْ خَلَقْتُ وَحِيدًا",
          "numberInSurah": 11,
          "juz": 29
        },
        {
          "number": 5507,
          "text": "وَجَعَلْتُ لَهُ مَالًا مَمْدُودًا",
          "numberInSurah": 12,
          "juz": 29
        },
        {
          "number": 5508,
          "text": "وَبَنِينَ شُهُودًا",
          "numberInSurah": 13,
          "juz": 29
        },
        {
          "number": 5509,
          "text": "وَمَهَّدْتُ لَهُ تَمْهِيدًا",
          "numberInSurah": 14,
          "juz": 29
        },
        {
          "number": 5510,
          "text": "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ",
          "numberInSurah": 15,
          "juz": 29
        },
        {
          "number": 5511,
          "text": "كَلَّا ۖ إِنَّهُ كَانَ لِآيَاتِنَا عَنِيدًا",
          "numberInSurah": 16,
          "juz": 29
        },
        {
          "number": 5512,
          "text": "سَأُرْهِقُهُ صَعُودًا",
          "numberInSurah": 17,
          "juz": 29
        },
        {
          "number": 5513,
          "text": "إِنَّهُ فَكَّرَ وَقَدَّرَ",
          "numberInSurah": 18,
          "juz": 29
        },
        {
          "number": 5514,
          "text": "فَقُتِلَ كَيْفَ قَدَّرَ",
          "numberInSurah": 19,
          "juz": 29
        },
        {
          "number": 5515,
          "text": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ",
          "numberInSurah": 20,
          "juz": 29
        },
        {
          "number": 5516,
          "text": "ثُمَّ نَظَرَ",
          "numberInSurah": 21,
          "juz": 29
        },
        {
          "number": 5517,
          "text": "ثُمَّ عَبَسَ وَبَسَرَ",
          "numberInSurah": 22,
          "juz": 29
        },
        {
          "number": 5518,
          "text": "ثُمَّ أَدْبَرَ وَاسْتَكْبَرَ",
          "numberInSurah": 23,
          "juz": 29
        },
        {
          "number": 5519,
          "text": "فَقَالَ إِنْ هَٰذَا إِلَّا سِحْرٌ يُؤْثَرُ",
          "numberInSurah": 24,
          "juz": 29
        },
        {
          "number": 5520,
          "text": "إِنْ هَٰذَا إِلَّا قَوْلُ الْبَشَرِ",
          "numberInSurah": 25,
          "juz": 29
        },
        {
          "number": 5521,
          "text": "سَأُصْلِيهِ سَقَرَ",
          "numberInSurah": 26,
          "juz": 29
        },
        {
          "number": 5522,
          "text": "وَمَا أَدْرَاكَ مَا سَقَرُ",
          "numberInSurah": 27,
          "juz": 29
        },
        {
          "number": 5523,
          "text": "لَا تُبْقِي وَلَا تَذَرُ",
          "numberInSurah": 28,
          "juz": 29
        },
        {
          "number": 5524,
          "text": "لَوَّاحَةٌ لِلْبَشَرِ",
          "numberInSurah": 29,
          "juz": 29
        },
        {
          "number": 5525,
          "text": "عَلَيْهَا تِسْعَةَ عَشَرَ",
          "numberInSurah": 30,
          "juz": 29
        },
        {
          "number": 5526,
          "text": "وَمَا جَعَلْنَا أَصْحَابَ النَّارِ إِلَّا مَلَائِكَةً ۙ وَمَا جَعَلْنَا عِدَّتَهُمْ إِلَّا فِتْنَةً لِلَّذِينَ كَفَرُوا لِيَسْتَيْقِنَ الَّذِينَ أُوتُوا الْكِتَابَ وَيَزْدَادَ الَّذِينَ آمَنُوا إِيمَانًا ۙ وَلَا يَرْتَابَ الَّذِينَ أُوتُوا الْكِتَابَ وَالْمُؤْمِنُونَ ۙ وَلِيَقُولَ الَّذِينَ فِي قُلُوبِهِمْ مَرَضٌ وَالْكَافِرُونَ مَاذَا أَرَادَ اللَّهُ بِهَٰذَا مَثَلًا ۚ كَذَٰلِكَ يُضِلُّ اللَّهُ مَنْ يَشَاءُ وَيَهْدِي مَنْ يَشَاءُ ۚ وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَ ۚ وَمَا هِيَ إِلَّا ذِكْرَىٰ لِلْبَشَرِ",
          "numberInSurah": 31,
          "juz": 29
        },
        {
          "number": 5527,
          "text": "كَلَّا وَالْقَمَرِ",
          "numberInSurah": 32,
          "juz": 29
        },
        {
          "number": 5528,
          "text": "وَاللَّيْلِ إِذْ أَدْبَرَ",
          "numberInSurah": 33,
          "juz": 29
        },
        {
          "number": 5529,
          "text": "وَالصُّبْحِ إِذَا أَسْفَرَ",
          "numberInSurah": 34,
          "juz": 29
        },
        {
          "number": 5530,
          "text": "إِنَّهَا لَإِحْدَى الْكُبَرِ",
          "numberInSurah": 35,
          "juz": 29
        },
        {
          "number": 5531,
          "text": "نَذِيرًا لِلْبَشَرِ",
          "numberInSurah": 36,
          "juz": 29
        },
        {
          "number": 5532,
          "text": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَتَقَدَّمَ أَوْ يَتَأَخَّرَ",
          "numberInSurah": 37,
          "juz": 29
        },
        {
          "number": 5533,
          "text": "كُلُّ نَفْسٍ بِمَا كَسَبَتْ رَهِينَةٌ",
          "numberInSurah": 38,
          "juz": 29
        },
        {
          "number": 5534,
          "text": "إِلَّا أَصْحَابَ الْيَمِينِ",
          "numberInSurah": 39,
          "juz": 29
        },
        {
          "number": 5535,
          "text": "فِي جَنَّاتٍ يَتَسَاءَلُونَ",
          "numberInSurah": 40,
          "juz": 29
        },
        {
          "number": 5536,
          "text": "عَنِ الْمُجْرِمِينَ",
          "numberInSurah": 41,
          "juz": 29
        },
        {
          "number": 5537,
          "text": "مَا سَلَكَكُمْ فِي سَقَرَ",
          "numberInSurah": 42,
          "juz": 29
        },
        {
          "number": 5538,
          "text": "قَالُوا لَمْ نَكُ مِنَ الْمُصَلِّينَ",
          "numberInSurah": 43,
          "juz": 29
        },
        {
          "number": 5539,
          "text": "وَلَمْ نَكُ نُطْعِمُ الْمِسْكِينَ",
          "numberInSurah": 44,
          "juz": 29
        },
        {
          "number": 5540,
          "text": "وَكُنَّا نَخُوضُ مَعَ الْخَائِضِينَ",
          "numberInSurah": 45,
          "juz": 29
        },
        {
          "number": 5541,
          "text": "وَكُنَّا نُكَذِّبُ بِيَوْمِ الدِّينِ",
          "numberInSurah": 46,
          "juz": 29
        },
        {
          "number": 5542,
          "text": "حَتَّىٰ أَتَانَا الْيَقِينُ",
          "numberInSurah": 47,
          "juz": 29
        },
        {
          "number": 5543,
          "text": "فَمَا تَنْفَعُهُمْ شَفَاعَةُ الشَّافِعِينَ",
          "numberInSurah": 48,
          "juz": 29
        },
        {
          "number": 5544,
          "text": "فَمَا لَهُمْ عَنِ التَّذْكِرَةِ مُعْرِضِينَ",
          "numberInSurah": 49,
          "juz": 29
        },
        {
          "number": 5545,
          "text": "كَأَنَّهُمْ حُمُرٌ مُسْتَنْفِرَةٌ",
          "numberInSurah": 50,
          "juz": 29
        },
        {
          "number": 5546,
          "text": "فَرَّتْ مِنْ قَسْوَرَةٍ",
          "numberInSurah": 51,
          "juz": 29
        },
        {
          "number": 5547,
          "text": "بَلْ يُرِيدُ كُلُّ امْرِئٍ مِنْهُمْ أَنْ يُؤْتَىٰ صُحُفًا مُنَشَّرَةً",
          "numberInSurah": 52,
          "juz": 29
        },
        {
          "number": 5548,
          "text": "كَلَّا ۖ بَلْ لَا يَخَافُونَ الْآخِرَةَ",
          "numberInSurah": 53,
          "juz": 29
        },
        {
          "number": 5549,
          "text": "كَلَّا إِنَّهُ تَذْكِرَةٌ",
          "numberInSurah": 54,
          "juz": 29
        },
        {
          "number": 5550,
          "text": "فَمَنْ شَاءَ ذَكَرَهُ",
          "numberInSurah": 55,
          "juz": 29
        },
        {
          "number": 5551,
          "text": "وَمَا يَذْكُرُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ ۚ هُوَ أَهْلُ التَّقْوَىٰ وَأَهْلُ الْمَغْفِرَةِ",
          "numberInSurah": 56,
          "juz": 29
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
        "format": "text",
        "type": "quran"
      }
    },
    {
      "number": 74,
      "name": "سورة المدثر",
      "englishName": "Al-Muddaththir",
      "englishNameTranslation": "The Cloaked One",
      "revelationType": "Meccan",
      "numberOfAyahs": 56,
      "ayahs": [
        {
          "text": "In the name of God, The Most Gracious, The Dispenser of Grace:"
        },
        {
          "number": 5496,
          "text": "O THOU [in thy solitude] enfolded!",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5497,
          "text": "Arise and warn!",
          "numberInSurah": 2,
          "juz": 29
        },
        {
          "number": 5498,
          "text": "And thy Sustainer's greatness glorify!",
          "numberInSurah": 3,
          "juz": 29
        },
        {
          "number": 5499,
          "text": "And thine inner self purify!",
          "numberInSurah": 4,
          "juz": 29
        },
        {
          "number": 5500,
          "text": "And all defilement shun!",
          "numberInSurah": 5,
          "juz": 29
        },
        {
          "number": 5501,
          "text": "And do not through giving seek thyself to gain,",
          "numberInSurah": 6,
          "juz": 29
        },
        {
          "number": 5502,
          "text": "but unto thy Sustainer turn in patience.",
          "numberInSurah": 7,
          "juz": 29
        },
        {
          "number": 5503,
          "text": "And [warn all men that] when the trumpet-call [of resurrection] is sounded,",
          "numberInSurah": 8,
          "juz": 29
        },
        {
          "number": 5504,
          "text": "that very Day shall be a day of anguish,",
          "numberInSurah": 9,
          "juz": 29
        },
        {
          "number": 5505,
          "text": "not of ease, for all who [now] deny the truth!",
          "numberInSurah": 10,
          "juz": 29
        },
        {
          "number": 5506,
          "text": "LEAVE Me alone [to deal] with him whom I have created alone,",
          "numberInSurah": 11,
          "juz": 29
        },
        {
          "number": 5507,
          "text": "and to whom I have granted resources vast,",
          "numberInSurah": 12,
          "juz": 29
        },
        {
          "number": 5508,
          "text": "and children as [love's] witnesses,",
          "numberInSurah": 13,
          "juz": 29
        },
        {
          "number": 5509,
          "text": "and to whose life I gave so wide a scope:",
          "numberInSurah": 14,
          "juz": 29
        },
        {
          "number": 5510,
          "text": "and yet, he greedily desires that I give yet more!",
          "numberInSurah": 15,
          "juz": 29
        },
        {
          "number": 5511,
          "text": "Nay, verily, it is against Our messages that he knowingly, stubbornly sets himself",
          "numberInSurah": 16,
          "juz": 29
        },
        {
          "number": 5512,
          "text": "[and so] I shall constrain him to endure a painful uphill climb!",
          "numberInSurah": 17,
          "juz": 29
        },
        {
          "number": 5513,
          "text": "Behold, [when Our messages are conveyed to one who is bent on denying the truth,] he reflects and meditates [as to how to disprove them] –",
          "numberInSurah": 18,
          "juz": 29
        },
        {
          "number": 5514,
          "text": "and thus he destroys himself, the way he meditates:",
          "numberInSurah": 19,
          "juz": 29
        },
        {
          "number": 5515,
          "text": "yea, he destroys himself, the way he meditates!",
          "numberInSurah": 20,
          "juz": 29
        },
        {
          "number": 5516,
          "text": "and then he looks [around for new arguments],",
          "numberInSurah": 21,
          "juz": 29
        },
        {
          "number": 5517,
          "text": "and then he frowns and glares,",
          "numberInSurah": 22,
          "juz": 29
        },
        {
          "number": 5518,
          "text": "and in the end he turns his back [on Our message], and glories in his arrogance,",
          "numberInSurah": 23,
          "juz": 29
        },
        {
          "number": 5519,
          "text": "and says, \"All this is mere spellbinding eloquence handed down [from olden times]!",
          "numberInSurah": 24,
          "juz": 29
        },
        {
          "number": 5520,
          "text": "This is nothing but the word of mortal man!\"",
          "numberInSurah": 25,
          "juz": 29
        },
        {
          "number": 5521,
          "text": "[Hence,] I shall cause him to endure hell-fire [in the life to come]!",
          "numberInSurah": 26,
          "juz": 29
        },
        {
          "number": 5522,
          "text": "And what could make thee conceive what hell-fire is?",
          "numberInSurah": 27,
          "juz": 29
        },
        {
          "number": 5523,
          "text": "It does not allow to live, and neither leaves [to die],",
          "numberInSurah": 28,
          "juz": 29
        },
        {
          "number": 5524,
          "text": "making [all truth] visible to mortal man.",
          "numberInSurah": 29,
          "juz": 29
        },
        {
          "number": 5525,
          "text": "Over it are nineteen [powers].",
          "numberInSurah": 30,
          "juz": 29
        },
        {
          "number": 5526,
          "text": "For We have caused none but angelic powers to lord over the fire [of hell]; and We have not caused their number to be aught but a trial for those who are bent on denying the truth - to the end that they who have been granted revelation aforetime might be convinced [of the truth of this divine writ]; and that they who have attained to faith [in it] might grow yet more firm in their faith; and that [both] they who have been granted the earlier revelation and they who believe [in this one] might be freed of all doubt; and that they in whose hearts is disease and the who deny the truth outright might ask, \"What does [your] God mean by this parable?\" In this way God lets go astray him that wills [to go astray], and guides aright him that wills [to be guided]. And none can comprehend thy Sustainers forces save Him alone: and all this is but a reminder to mortal man.",
          "numberInSurah": 31,
          "juz": 29
        },
        {
          "number": 5527,
          "text": "NAY, but consider the moon!",
          "numberInSurah": 32,
          "juz": 29
        },
        {
          "number": 5528,
          "text": "Consider the night when it departs,",
          "numberInSurah": 33,
          "juz": 29
        },
        {
          "number": 5529,
          "text": "and the morn when it dawns!",
          "numberInSurah": 34,
          "juz": 29
        },
        {
          "number": 5530,
          "text": "Verily, that [hell-fire) is Indeed one of the great [forewarnings] –",
          "numberInSurah": 35,
          "juz": 29
        },
        {
          "number": 5531,
          "text": "a warning to mortal man –",
          "numberInSurah": 36,
          "juz": 29
        },
        {
          "number": 5532,
          "text": "to everyone of you, whether he chooses to come forward or to hang back!",
          "numberInSurah": 37,
          "juz": 29
        },
        {
          "number": 5533,
          "text": "[On the Day of Judgment,] every human being will be held in pledge for whatever [evil] he has wrought –",
          "numberInSurah": 38,
          "juz": 29
        },
        {
          "number": 5534,
          "text": "save those who shall have attained to righteousness:",
          "numberInSurah": 39,
          "juz": 29
        },
        {
          "number": 5535,
          "text": "[dwelling] In gardens [of paradise], they will inquire",
          "numberInSurah": 40,
          "juz": 29
        },
        {
          "number": 5536,
          "text": "of those who were lost in sin:",
          "numberInSurah": 41,
          "juz": 29
        },
        {
          "number": 5537,
          "text": "\"What has brought you into hell-fire?\"",
          "numberInSurah": 42,
          "juz": 29
        },
        {
          "number": 5538,
          "text": "They will answer: \"We were not among those who prayed;",
          "numberInSurah": 43,
          "juz": 29
        },
        {
          "number": 5539,
          "text": "and neither did we feed the needy;",
          "numberInSurah": 44,
          "juz": 29
        },
        {
          "number": 5540,
          "text": "and we were wont to indulge in sinning together with all [the others] who indulged in it;",
          "numberInSurah": 45,
          "juz": 29
        },
        {
          "number": 5541,
          "text": "and the Day of Judgment we were wont to call a lie –",
          "numberInSurah": 46,
          "juz": 29
        },
        {
          "number": 5542,
          "text": "until certainty came upon us [in death].\"",
          "numberInSurah": 47,
          "juz": 29
        },
        {
          "number": 5543,
          "text": "And so, of no benefit to them could be the intercession of any that would intercede for them.",
          "numberInSurah": 48,
          "juz": 29
        },
        {
          "number": 5544,
