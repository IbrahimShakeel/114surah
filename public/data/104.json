      "name": "سورة الهمزة",
      "englishName": "Al-Humaza",
      "englishNameTranslation": "The Traducer",
      "revelationType": "Meccan",
      "numberOfAyahs": 9,
      "ayahs": [
        {
          "number": 6180,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6180,
          "text": "وَيْلٌ لِكُلِّ هُمَزَةٍ لُمَزَةٍ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6181,
          "text": "الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6182,
          "text": "يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6183,
          "text": "كَلَّا ۖ لَيُنْبَذَنَّ فِي الْحُطَمَةِ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6184,
          "text": "وَمَا أَدْرَاكَ مَا الْحُطَمَةُ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6185,
          "text": "نَارُ اللَّهِ الْمُوقَدَةُ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6186,
          "text": "الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6187,
          "text": "إِنَّهَا عَلَيْهِمْ مُؤْصَدَةٌ",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 6188,
          "text": "فِي عَمَدٍ مُمَدَّدَةٍ",
          "numberInSurah": 9,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
