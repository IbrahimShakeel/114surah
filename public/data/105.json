          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6189,
          "text": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَابِ الْفِيلِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6190,
          "text": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِي تَضْلِيلٍ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6191,
          "text": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
