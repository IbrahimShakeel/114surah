  "data": [
    {
      "number": 81,
      "name": "سورة التكوير",
      "englishName": "At-Takwir",
      "englishNameTranslation": "The Overthrowing",
      "revelationType": "Meccan",
      "numberOfAyahs": 29,
      "ayahs": [
        {
          "number": 5801,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 5801,
          "text": "إِذَا الشَّمْسُ كُوِّرَتْ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 5802,
          "text": "وَإِذَا النُّجُومُ انْكَدَرَتْ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 5803,
          "text": "وَإِذَا الْجِبَالُ سُيِّرَتْ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 5804,
          "text": "وَإِذَا الْعِشَارُ عُطِّلَتْ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 5805,
          "text": "وَإِذَا الْوُحُوشُ حُشِرَتْ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 5806,
          "text": "وَإِذَا الْبِحَارُ سُجِّرَتْ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 5807,
          "text": "وَإِذَا النُّفُوسُ زُوِّجَتْ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 5808,
          "text": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 5809,
          "text": "بِأَيِّ ذَنْبٍ قُتِلَتْ",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 5810,
          "text": "وَإِذَا الصُّحُفُ نُشِرَتْ",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 5811,
          "text": "وَإِذَا السَّمَاءُ كُشِطَتْ",
          "numberInSurah": 11,
          "juz": 30
        },
        {
          "number": 5812,
          "text": "وَإِذَا الْجَحِيمُ سُعِّرَتْ",
          "numberInSurah": 12,
          "juz": 30
        },
        {
          "number": 5813,
          "text": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ",
          "numberInSurah": 13,
          "juz": 30
        },
        {
          "number": 5814,
          "text": "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ",
          "numberInSurah": 14,
          "juz": 30
        },
        {
          "number": 5815,
          "text": "فَلَا أُقْسِمُ بِالْخُنَّسِ",
          "numberInSurah": 15,
          "juz": 30
        },
        {
          "number": 5816,
          "text": "الْجَوَارِ الْكُنَّسِ",
          "numberInSurah": 16,
          "juz": 30
        },
        {
          "number": 5817,
          "text": "وَاللَّيْلِ إِذَا عَسْعَسَ",
          "numberInSurah": 17,
          "juz": 30
        },
        {
          "number": 5818,
          "text": "وَالصُّبْحِ إِذَا تَنَفَّسَ",
          "numberInSurah": 18,
          "juz": 30
        },
        {
          "number": 5819,
          "text": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ",
          "numberInSurah": 19,
          "juz": 30
        },
        {
          "number": 5820,
          "text": "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ",
          "numberInSurah": 20,
          "juz": 30
        },
        {
          "number": 5821,
          "text": "مُطَاعٍ ثَمَّ أَمِينٍ",
          "numberInSurah": 21,
          "juz": 30
        },
        {
          "number": 5822,
          "text": "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ",
          "numberInSurah": 22,
          "juz": 30
        },
        {
          "number": 5823,
          "text": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ",
          "numberInSurah": 23,
          "juz": 30
        },
        {
          "number": 5824,
          "text": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ",
          "numberInSurah": 24,
          "juz": 30
        },
        {
          "number": 5825,
          "text": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ",
          "numberInSurah": 25,
          "juz": 30
        },
        {
          "number": 5826,
          "text": "فَأَيْنَ تَذْهَبُونَ",
          "numberInSurah": 26,
          "juz": 30
        },
        {
          "number": 5827,
          "text": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ",
          "numberInSurah": 27,
          "juz": 30
        },
        {
          "number": 5828,
          "text": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ",
          "numberInSurah": 28,
          "juz": 30
        },
        {
          "number": 5829,
          "text": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ",
          "numberInSurah": 29,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
        "format": "text",
        "type": "quran"
      }
    },
    {
      "number": 81,
      "name": "سورة التكوير",
      "englishName": "At-Takwir",
      "englishNameTranslation": "The Overthrowing",
      "revelationType": "Meccan",
      "numberOfAyahs": 29,
      "ayahs": [
        {
          "text": "In the name of God, The Most Gracious, The Dispenser of Grace:"
        },
        {
          "number": 5801,
          "text": "WHEN THE SUN is shrouded in darkness,",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 5802,
          "text": "and when the stars lose their light,",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 5803,
          "text": "and when the mountains are made to vanish,",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 5804,
          "text": "and when she-camels big with young, about to give birth, are left untended,",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 5805,
          "text": "and when all beasts are gathered together,",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 5806,
          "text": "and when the seas boil over,",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 5807,
          "text": "and when all human beings are coupled [with their deeds],",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 5808,
          "text": "and when the girl-child that was buried alive is made to ask",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 5809,
          "text": "for what crime she had been slain,",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 5810,
          "text": "and when the scrolls [of men's deeds] are unfolded,",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 5811,
          "text": "and when heaven is laid bare,",
          "numberInSurah": 11,
          "juz": 30
        },
        {
          "number": 5812,
          "text": "and when the blazing fire [of hell] is kindled bright,",
          "numberInSurah": 12,
          "juz": 30
        },
        {
          "number": 5813,
          "text": "and when paradise is brought into view:",
          "numberInSurah": 13,
          "juz": 30
        },
        {
          "number": 5814,
          "text": "[on that Day] every human being will come to know what he has prepared [for himself].",
          "numberInSurah": 14,
          "juz": 30
        },
        {
          "number": 5815,
          "text": "BUT NAY! I call to witness the revolving stars,",
          "numberInSurah": 15,
          "juz": 30
        },
        {
          "number": 5816,
          "text": "the planets that run their course and set,",
          "numberInSurah": 16,
          "juz": 30
        },
        {
          "number": 5817,
          "text": "and the night as it darkly falls,",
          "numberInSurah": 17,
          "juz": 30
        },
        {
          "number": 5818,
          "text": "and the morn as it softly breathes:",
          "numberInSurah": 18,
          "juz": 30
        },
        {
          "number": 5819,
          "text": "behold, this [divine writ] is indeed the [inspired] word of a noble apostle,",
          "numberInSurah": 19,
          "juz": 30
        },
        {
          "number": 5820,
          "text": "with strength endowed, secure with Him who in almightiness is enthroned",
          "numberInSurah": 20,
          "juz": 30
        },
        {
          "number": 5821,
          "text": "[the word] of one to be heeded, and worthy of trust!",
          "numberInSurah": 21,
          "juz": 30
        },
        {
          "number": 5822,
          "text": "For, this fellow-man of yours is not a madman:",
          "numberInSurah": 22,
          "juz": 30
        },
        {
          "number": 5823,
          "text": "he truly beheld [the angel - beheld] him on the clear horizon;",
          "numberInSurah": 23,
          "juz": 30
        },
        {
          "number": 5824,
          "text": "and he is not one to begrudge others the knowledge [of whatever has been revealed to him] out of that which is beyond the reach of human Perception.",
          "numberInSurah": 24,
          "juz": 30
        },
        {
          "number": 5825,
          "text": "Nor is this [message] the word of any satanic force accursed.",
          "numberInSurah": 25,
          "juz": 30
        },
        {
          "number": 5826,
          "text": "Whither, then, will you go?",
          "numberInSurah": 26,
          "juz": 30
        },
        {
          "number": 5827,
          "text": "This [message] is no less than a reminder to all mankind –",
          "numberInSurah": 27,
          "juz": 30
        },
        {
          "number": 5828,
          "text": "to everyone of you who wills to walk a straight way.",
          "numberInSurah": 28,
          "juz": 30
        },
        {
          "number": 5829,
          "text": "But you cannot will it unless God, the Sustainer of all the worlds, wills [to show you that way].",
          "numberInSurah": 29,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "en.asad",
        "language": "en",
        "name": "Asad",
        "englishName": "Muhammad Asad",
        "format": "text",
        "type": "translation"
      }
    }
  ]
}