    {
      "number": 93,
      "name": "سورة الضحى",
      "englishName": "Ad-Dhuhaa",
      "englishNameTranslation": "The Morning Hours",
      "revelationType": "Meccan",
      "numberOfAyahs": 11,
      "ayahs": [
        {
          "number": 6080,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6080,
          "text": "وَالضُّحَىٰ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6081,
          "text": "وَاللَّيْلِ إِذَا سَجَىٰ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6082,
          "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6083,
          "text": "وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَىٰ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6084,
          "text": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6085,
          "text": "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6086,
          "text": "وَوَجَدَكَ ضَالًّا فَهَدَىٰ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6087,
          "text": "وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 6088,
          "text": "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 6089,
          "text": "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 6090,
          "text": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
          "numberInSurah": 11,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
