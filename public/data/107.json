      "name": "سورة الماعون",
      "englishName": "Al-Maa'un",
      "englishNameTranslation": "Almsgiving",
      "revelationType": "Meccan",
      "numberOfAyahs": 7,
      "ayahs": [
        {
          "number": 6198,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6198,
          "text": "أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6199,
          "text": "فَذَٰلِكَ الَّذِي يَدُعُّ الْيَتِيمَ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6200,
          "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6201,
          "text": "فَوَيْلٌ لِلْمُصَلِّينَ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6202,
          "text": "الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6203,
          "text": "الَّذِينَ هُمْ يُرَاءُونَ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6204,
          "text": "وَيَمْنَعُونَ الْمَاعُونَ",
          "numberInSurah": 7,
          "juz": 30
        }
      ],
      "edition": {
