    {
      "number": 96,
      "name": "سورة العلق",
      "englishName": "Al-Alaq",
      "englishNameTranslation": "The Clot",
      "revelationType": "Meccan",
      "numberOfAyahs": 19,
      "ayahs": [
        {
          "number": 6107,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6107,
          "text": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6108,
          "text": "خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6109,
          "text": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6110,
          "text": "الَّذِي عَلَّمَ بِالْقَلَمِ",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6111,
          "text": "عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6112,
          "text": "كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَىٰ",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6113,
          "text": "أَنْ رَآهُ اسْتَغْنَىٰ",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6114,
          "text": "إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 6115,
          "text": "أَرَأَيْتَ الَّذِي يَنْهَىٰ",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 6116,
          "text": "عَبْدًا إِذَا صَلَّىٰ",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 6117,
          "text": "أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَىٰ",
          "numberInSurah": 11,
          "juz": 30
        },
        {
          "number": 6118,
          "text": "أَوْ أَمَرَ بِالتَّقْوَىٰ",
          "numberInSurah": 12,
          "juz": 30
        },
        {
          "number": 6119,
          "text": "أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّىٰ",
          "numberInSurah": 13,
          "juz": 30
        },
        {
          "number": 6120,
          "text": "أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَىٰ",
          "numberInSurah": 14,
          "juz": 30
        },
        {
          "number": 6121,
          "text": "كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ",
          "numberInSurah": 15,
          "juz": 30
        },
        {
          "number": 6122,
          "text": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ",
          "numberInSurah": 16,
          "juz": 30
        },
        {
          "number": 6123,
          "text": "فَلْيَدْعُ نَادِيَهُ",
          "numberInSurah": 17,
          "juz": 30
        },
        {
          "number": 6124,
          "text": "سَنَدْعُ الزَّبَانِيَةَ",
          "numberInSurah": 18,
          "juz": 30
        },
        {
          "number": 6125,
          "text": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ۩",
          "numberInSurah": 19,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
        "format": "text",
        "type": "quran"
      }
    },
    {
      "number": 96,
      "name": "سورة العلق",
      "englishName": "Al-Alaq",
      "englishNameTranslation": "The Clot",
      "revelationType": "Meccan",
      "numberOfAyahs": 19,
      "ayahs": [
        {
          "text": "In the name of God, The Most Gracious, The Dispenser of Grace:"
        },
        {
          "number": 6107,
          "text": "READ in the name of thy Sustainer, who has-",
          "numberInSurah": 1,
          "juz": 30
        },
        {
          "number": 6108,
          "text": "created man out of a germ-cell",
          "numberInSurah": 2,
          "juz": 30
        },
        {
          "number": 6109,
          "text": "Read - for thy Sustainer is the Most Bountiful One",
          "numberInSurah": 3,
          "juz": 30
        },
        {
          "number": 6110,
          "text": "who has taught [man] the use of the pen –",
          "numberInSurah": 4,
          "juz": 30
        },
        {
          "number": 6111,
          "text": "taught man what he did not know!",
          "numberInSurah": 5,
          "juz": 30
        },
        {
          "number": 6112,
          "text": "Nay, verily, man becomes grossly overweening",
          "numberInSurah": 6,
          "juz": 30
        },
        {
          "number": 6113,
          "text": "whenever he believes himself to be self-sufficient:",
          "numberInSurah": 7,
          "juz": 30
        },
        {
          "number": 6114,
          "text": "for, behold, unto thy Sustainer all must return.",
          "numberInSurah": 8,
          "juz": 30
        },
        {
          "number": 6115,
          "text": "HAST THOU ever considered him who tries to prevent",
          "numberInSurah": 9,
          "juz": 30
        },
        {
          "number": 6116,
          "text": "a servant [of God] from praying?",
          "numberInSurah": 10,
          "juz": 30
        },
        {
          "number": 6117,
          "text": "Hast thou considered whether he is on the right way,",
          "numberInSurah": 11,
          "juz": 30
        },
        {
          "number": 6118,
          "text": "or is concerned with God-consciousness?",
          "numberInSurah": 12,
          "juz": 30
        },
        {
          "number": 6119,
          "text": "Hast thou considered whether he may [not] be giving the lie to the truth and turning his back [upon it]?",
          "numberInSurah": 13,
          "juz": 30
        },
        {
          "number": 6120,
          "text": "Does he, then, not know that God sees [all]?",
          "numberInSurah": 14,
          "juz": 30
        },
        {
          "number": 6121,
          "text": "Nay, if he desist not, We shall most surely drag him down upon his forehead",
          "numberInSurah": 15,
          "juz": 30
        },
        {
          "number": 6122,
          "text": "the lying, rebellious forehead! –",
          "numberInSurah": 16,
          "juz": 30
        },
        {
          "number": 6123,
          "text": "and then let him summon [to his aid] the counsels of his own [spurious] wisdom,",
          "numberInSurah": 17,
          "juz": 30
        },
        {
          "number": 6124,
          "text": "[the while] We shall summon the forces of heavenly chastisement!",
          "numberInSurah": 18,
          "juz": 30
        },
        {
          "number": 6125,
          "text": "Nay, pay thou no heed to him, but prostrate thyself [before God] and draw close [unto Him]!",
          "numberInSurah": 19,
          "juz": 30
        }
      ],
      "edition": {
        "identifier": "en.asad",
        "language": "en",
        "name": "Asad",
        "englishName": "Muhammad Asad",
        "format": "text",
        "type": "translation"
      }
    }
  ]
}