  "data": [
    {
      "number": 77,
      "name": "سورة المرسلات",
      "englishName": "Al-Mursalaat",
      "englishNameTranslation": "The Emissaries",
      "revelationType": "Meccan",
      "numberOfAyahs": 50,
      "ayahs": [
        {
          "number": 5623,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5623,
          "text": "وَالْمُرْسَلَاتِ عُرْفًا",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5624,
          "text": "فَالْعَاصِفَاتِ عَصْفًا",
          "numberInSurah": 2,
          "juz": 29
        },
        {
          "number": 5625,
          "text": "وَالنَّاشِرَاتِ نَشْرًا",
          "numberInSurah": 3,
          "juz": 29
        },
        {
          "number": 5626,
          "text": "فَالْفَارِقَاتِ فَرْقًا",
          "numberInSurah": 4,
          "juz": 29
        },
        {
          "number": 5627,
          "text": "فَالْمُلْقِيَاتِ ذِكْرًا",
          "numberInSurah": 5,
          "juz": 29
        },
        {
          "number": 5628,
          "text": "عُذْرًا أَوْ نُذْرًا",
          "numberInSurah": 6,
          "juz": 29
        },
        {
          "number": 5629,
          "text": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ",
          "numberInSurah": 7,
          "juz": 29
        },
        {
          "number": 5630,
          "text": "فَإِذَا النُّجُومُ طُمِسَتْ",
          "numberInSurah": 8,
          "juz": 29
        },
        {
          "number": 5631,
          "text": "وَإِذَا السَّمَاءُ فُرِجَتْ",
          "numberInSurah": 9,
          "juz": 29
        },
        {
          "number": 5632,
          "text": "وَإِذَا الْجِبَالُ نُسِفَتْ",
          "numberInSurah": 10,
          "juz": 29
        },
        {
          "number": 5633,
          "text": "وَإِذَا الرُّسُلُ أُقِّتَتْ",
          "numberInSurah": 11,
          "juz": 29
        },
        {
          "number": 5634,
          "text": "لِأَيِّ يَوْمٍ أُجِّلَتْ",
          "numberInSurah": 12,
          "juz": 29
        },
        {
          "number": 5635,
          "text": "لِيَوْمِ الْفَصْلِ",
          "numberInSurah": 13,
          "juz": 29
        },
        {
          "number": 5636,
          "text": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ",
          "numberInSurah": 14,
          "juz": 29
        },
        {
          "number": 5637,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 15,
          "juz": 29
        },
        {
          "number": 5638,
          "text": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ",
          "numberInSurah": 16,
          "juz": 29
        },
        {
          "number": 5639,
          "text": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ",
          "numberInSurah": 17,
          "juz": 29
        },
        {
          "number": 5640,
          "text": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ",
          "numberInSurah": 18,
          "juz": 29
        },
        {
          "number": 5641,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 19,
          "juz": 29
        },
        {
          "number": 5642,
          "text": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ",
          "numberInSurah": 20,
          "juz": 29
        },
        {
          "number": 5643,
          "text": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ",
          "numberInSurah": 21,
          "juz": 29
        },
        {
          "number": 5644,
          "text": "إِلَىٰ قَدَرٍ مَعْلُومٍ",
          "numberInSurah": 22,
          "juz": 29
        },
        {
          "number": 5645,
          "text": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ",
          "numberInSurah": 23,
          "juz": 29
        },
        {
          "number": 5646,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 24,
          "juz": 29
        },
        {
          "number": 5647,
          "text": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا",
          "numberInSurah": 25,
          "juz": 29
        },
        {
          "number": 5648,
          "text": "أَحْيَاءً وَأَمْوَاتًا",
          "numberInSurah": 26,
          "juz": 29
        },
        {
          "number": 5649,
          "text": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُمْ مَاءً فُرَاتًا",
          "numberInSurah": 27,
          "juz": 29
        },
        {
          "number": 5650,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 28,
          "juz": 29
        },
        {
          "number": 5651,
          "text": "انْطَلِقُوا إِلَىٰ مَا كُنْتُمْ بِهِ تُكَذِّبُونَ",
          "numberInSurah": 29,
          "juz": 29
        },
        {
          "number": 5652,
          "text": "انْطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ",
          "numberInSurah": 30,
          "juz": 29
        },
        {
          "number": 5653,
          "text": "لَا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ",
          "numberInSurah": 31,
          "juz": 29
        },
        {
          "number": 5654,
          "text": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ",
          "numberInSurah": 32,
          "juz": 29
        },
        {
          "number": 5655,
          "text": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ",
          "numberInSurah": 33,
          "juz": 29
        },
        {
          "number": 5656,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 34,
          "juz": 29
        },
        {
          "number": 5657,
          "text": "هَٰذَا يَوْمُ لَا يَنْطِقُونَ",
          "numberInSurah": 35,
          "juz": 29
        },
        {
          "number": 5658,
          "text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
          "numberInSurah": 36,
          "juz": 29
        },
        {
          "number": 5659,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 37,
          "juz": 29
        },
        {
          "number": 5660,
          "text": "هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ",
          "numberInSurah": 38,
          "juz": 29
        },
        {
          "number": 5661,
          "text": "فَإِنْ كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
          "numberInSurah": 39,
          "juz": 29
        },
        {
          "number": 5662,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 40,
          "juz": 29
        },
        {
          "number": 5663,
          "text": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ",
          "numberInSurah": 41,
          "juz": 29
        },
        {
          "number": 5664,
          "text": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ",
          "numberInSurah": 42,
          "juz": 29
        },
        {
          "number": 5665,
          "text": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ",
          "numberInSurah": 43,
          "juz": 29
        },
        {
          "number": 5666,
          "text": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ",
          "numberInSurah": 44,
          "juz": 29
        },
        {
          "number": 5667,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 45,
          "juz": 29
        },
        {
          "number": 5668,
          "text": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُمْ مُجْرِمُونَ",
          "numberInSurah": 46,
          "juz": 29
        },
        {
          "number": 5669,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 47,
          "juz": 29
        },
        {
          "number": 5670,
          "text": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ",
          "numberInSurah": 48,
          "juz": 29
        },
        {
          "number": 5671,
          "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
          "numberInSurah": 49,
          "juz": 29
        },
        {
          "number": 5672,
          "text": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ",
          "numberInSurah": 50,
          "juz": 29
        }
      ],
      "edition": {
        "identifier": "quran-simple",
        "language": "ar",
        "name": "Simple",
        "englishName": "Simple",
        "format": "text",
        "type": "quran"
      }
    },
    {
      "number": 77,
      "name": "سورة المرسلات",
      "englishName": "Al-Mursalaat",
      "englishNameTranslation": "The Emissaries",
      "revelationType": "Meccan",
      "numberOfAyahs": 50,
      "ayahs": [
        {
          "text": "In the name of God, The Most Gracious, The Dispenser of Grace:"
        },
        {
          "number": 5623,
          "text": "CONSIDER these [messages,] sent forth in waves",
          "numberInSurah": 1,
          "juz": 29
        },
        {
          "number": 5624,
          "text": "and then storming on with a tempest's force!",
          "numberInSurah": 2,
          "juz": 29
        },
        {
          "number": 5625,
          "text": "Consider these [messages] that spread [the truth] far and wide,",
          "numberInSurah": 3,
          "juz": 29
        },
        {
          "number": 5626,
          "text": "thus separating [right and wrong] with all clarity,",
          "numberInSurah": 4,
          "juz": 29
        },
        {
          "number": 5627,
          "text": "and then giving forth a reminder,",
          "numberInSurah": 5,
          "juz": 29
        },
        {
          "number": 5628,
          "text": "[promising] freedom from blame or [offering] a warning!",
          "numberInSurah": 6,
          "juz": 29
        },
        {
          "number": 5629,
          "text": "BEHOLD, all that you are told to expect will surely come to pass.",
          "numberInSurah": 7,
          "juz": 29
        },
        {
          "number": 5630,
          "text": "Thus, [it will come to pass] when the stars are effaced,",
          "numberInSurah": 8,
          "juz": 29
        },
        {
          "number": 5631,
          "text": "and when the sky is rent asunder,",
          "numberInSurah": 9,
          "juz": 29
        },
        {
          "number": 5632,
          "text": "and when the mountains are scattered like dust,",
          "numberInSurah": 10,
          "juz": 29
        },
        {
          "number": 5633,
          "text": "and when all the apostles are called together at a time appointed...",
          "numberInSurah": 11,
          "juz": 29
        },
        {
          "number": 5634,
          "text": "For what day has the term [of all this] been set?",
          "numberInSurah": 12,
          "juz": 29
        },
        {
          "number": 5635,
          "text": "For the Day of Distinction [between the true and the false]!",
          "numberInSurah": 13,
          "juz": 29
        },
        {
          "number": 5636,
          "text": "And what could make thee conceive what that Day of Distinction will be?",
          "numberInSurah": 14,
          "juz": 29
        },
        {
          "number": 5637,
          "text": "Woe on that Day unto those who give the lie to the truth!",
          "numberInSurah": 15,
          "juz": 29
        },
        {
          "number": 5638,
          "text": "Did We not destroy [so many of] those [sinners] of olden days?",
          "numberInSurah": 16,
          "juz": 29
        },
        {
          "number": 5639,
          "text": "And We shall let them be followed by those of later times:",
          "numberInSurah": 17,
          "juz": 29
        },
        {
          "number": 5640,
          "text": "[for] thus do We deal with such as are lost in sin.",
          "numberInSurah": 18,
          "juz": 29
        },
        {
          "number": 5641,
          "text": "Woe on that Day unto those who give the lie to the truth!",
          "numberInSurah": 19,
          "juz": 29
        },
        {
          "number": 5642,
          "text": "Did We not create you out of a humble fluid",
          "numberInSurah": 20,
          "juz": 29
        },
        {
          "number": 5643,
          "text": "which We then let remain in [the womb's] firm keeping",
          "numberInSurah": 21,
          "juz": 29
        },
        {
          "number": 5644,
          "text": "for a term pre-ordained?",
          "numberInSurah": 22,
          "juz": 29
        },
        {
          "number": 5645,
          "text": "Thus have We determined [the nature of man's creation]: and excellent indeed is Our power to determine [what is to be]!",
          "numberInSurah": 23,
          "juz": 29
        },
        {
          "number": 5646,
          "text": "Woe, on that Day unto those who give the lie to the truth!",
          "numberInSurah": 24,
          "juz": 29
        },
        {
          "number": 5647,
          "text": "Have We not caused the earth to hold within itself",
          "numberInSurah": 25,
          "juz": 29
        },
        {
          "number": 5648,
          "text": "the living and the dead?",
          "numberInSurah": 26,
          "juz": 29
        },
        {
          "number": 5649,
          "text": "and have We not set on it proud, firm mountains, and given you sweet water to drink?",
          "numberInSurah": 27,
          "juz": 29
        },
        {
          "number": 5650,
          "text": "Woe on that Day unto those who give the lie to the truth!",
          "numberInSurah": 28,
          "juz": 29
        },
        {
          "number": 5651,
          "text": "GO ON towards that [resurrection] which you were wont to call a lie!",
          "numberInSurah": 29,
          "juz": 29
        },
        {
          "number": 5652,
          "text": "Go on towards the threefold shadow",
          "numberInSurah": 30,
          "juz": 29
        },
        {
          "number": 5653,
          "text": "that will offer no [cooling] shade and will be of no avail against the flame",
          "numberInSurah": 31,
          "juz": 29
        },
        {
          "number": 5654,
          "text": "which – behold!- will throw up sparks like [burning] logs,",
          "numberInSurah": 32,
          "juz": 29
        },
        {
          "number": 5655,
          "text": "like giant fiery ropes!",
          "numberInSurah": 33,
          "juz": 29
        },
        {
          "number": 5656,
          "text": "Woe on that Day unto those who give the lie to the truth –",
          "numberInSurah": 34,
          "juz": 29
        },
        {
          "number": 5657,
          "text": "that Day on which they will not [be able to] utter a word,",
          "numberInSurah": 35,
          "juz": 29
        },
        {
          "number": 5658,
          "text": "nor be allowed to proffer excuses!",
          "numberInSurah": 36,
          "juz": 29
        },
        {
          "number": 5659,
          "text": "Woe on that Day unto those who give the lie to the truth –",
          "numberInSurah": 37,
          "juz": 29
        },
        {
          "number": 5660,
          "text": "that Day of Distinction [between the true and the false, when they will be told]: \"We have brought you together with those [sinners] of olden times;",
          "numberInSurah": 38,
          "juz": 29
        },
        {
          "number": 5661,
          "text": "and if you [think that you] have a subterfuge left, try to outwit Me!\"",
          "numberInSurah": 39,
          "juz": 29
        },
        {
          "number": 5662,
          "text": "Woe on that Day unto those who give the lie to the truth!",
          "numberInSurah": 40,
          "juz": 29
        },
        {
          "number": 5663,
          "text": "[AS AGAINST this,] behold, the God-conscious shall dwell amidst [cooling] shades and springs,",
          "numberInSurah": 41,
          "juz": 29
        },
        {
          "number": 5664,
          "text": "and [partake of] whatever fruit they may desire;",
          "numberInSurah": 42,
          "juz": 29
        },
        {
          "number": 5665,
          "text": "[and they will be told:] \"Eat and drink in good cheer in return for what you did [in life]!\"",
          "numberInSurah": 43,
          "juz": 29
        },
        {
          "number": 5666,
          "text": "Thus, behold, do We reward the doers of good;",
          "numberInSurah": 44,
          "juz": 29
        },
        {
          "number": 5667,
          "text": "[but] woe on that Day unto those who give the lie to the truth!",
          "numberInSurah": 45,
          "juz": 29
        },
        {
          "number": 5668,
          "text": "EAT [your fill] and enjoy your life for a little while, O you who are lost in sin!",
          "numberInSurah": 46,
          "juz": 29
        },
        {
          "number": 5669,
          "text": "[But] woe on that Day unto those who give the lie to the truth,",
          "numberInSurah": 47,
          "juz": 29
        },
        {
          "number": 5670,
          "text": "and when they are told, \"Bow down [before God]\", do not bow down:",
          "numberInSurah": 48,
          "juz": 29
        },
        {
          "number": 5671,
          "text": "woe on that Day unto those who give the to the truth!",
          "numberInSurah": 49,
          "juz": 29
        },
        {
          "number": 5672,
          "text": "In what other tiding, then, will they, after this, believe?",
          "numberInSurah": 50,
          "juz": 29
        }
      ],
      "edition": {
        "identifier": "en.asad",
        "language": "en",
        "name": "Asad",
        "englishName": "Muhammad Asad",
        "format": "text",
        "type": "translation"
      }
    }
  ]
}